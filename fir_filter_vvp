    %set/v v01122618_0, 0, 32;
T_1.2 ;
    %load/v 8, v01122618_0, 32;
   %cmpi/s 8, 4, 32;
    %jmp/0xz T_1.3, 5;
    %ix/getv/s 3, v01122618_0;
    %jmp/1 t_0, 4;
    %ix/load 0, 8, 0; word width
    %ix/load 1, 0, 0; part off
    %assign/av v010E3238, 0, 0;
t_0 ;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v01122618_0, 32;
    %set/v v01122618_0, 8, 32;
    %jmp T_1.2;
T_1.3 ;
    %jmp T_1.1;
T_1.0 ;
    %movi 8, 3, 32;
    %set/v v01122618_0, 8, 32;
T_1.4 ;
    %load/v 8, v01122618_0, 32;
    %cmp/s 0, 8, 32;
